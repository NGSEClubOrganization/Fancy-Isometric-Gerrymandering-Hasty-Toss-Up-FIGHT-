		user.getInventory().addItem(new ItemStack(Material.WATER, 1));
	}

	@Override
	public void effect(Player user) {
		// TODO Auto-generated method stub

	}

	@Override
	public ItemStack getItem() {
		// TODO Auto-generated method stub
		return null;
	}

}
